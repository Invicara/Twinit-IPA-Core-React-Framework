{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react", "react-hooks"],
    "rules": {
        "no-unused-vars": "off",
        "no-undef": "off"
    }
}  